<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PsychologyPracticeProblemApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PsychologyPracticeProblemApp"
    FlyoutBackground="white"
    >
    

    <FlyoutItem Title="Home">
        <ShellContent Title="Home" ContentTemplate="{DataTemplate local:HomePage}" Icon="dotnet_bot.png"/>
    </FlyoutItem>
    <FlyoutItem Title="Stats">
        <ShellContent Title="Problem" ContentTemplate="{DataTemplate local:StatsPage}" Icon="stats.png"/>
    </FlyoutItem>
    <FlyoutItem Title="History">
        <ShellContent Title="History" ContentTemplate="{DataTemplate local:History}" Icon="book.png"/>
    </FlyoutItem>

</Shell>
